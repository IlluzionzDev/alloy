{
  "name": "@alloy/core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "exports": {
    "./jsx-runtime": "./dist/src/jsx-runtime.js"
  },
  "scripts": {
    "build-src": "babel src -d dist/src --extensions '.ts,.tsx'",
    "build-test": "babel test -d dist/test --extensions '.ts,.tsx'",
    "build": "npm run build-src && npm run build-test",
    "watch-src": "babel src -d dist/src --extensions '.ts,.tsx' --watch",
    "watch-test": "babel test -d dist/test --extensions '.ts,.tsx' --watch",
    "watch": "concurrently --kill-others \"npm run watch-src\" \"npm run watch-test\""
  },
  "keywords": [],
