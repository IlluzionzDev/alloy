  "dependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@vue/reactivity": "^3.4.30",
    "babel-preset-alloy": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "concurrently": "^8.2.2",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "esbuild": "0.23"
  },
  "type": "module"
}
